{
	"name": "esmcss",
	"version": "0.1.8",
	"description": "Import .css.ts/.css.js functions & focused css helper classes from ES Modules using esbuild",
	"keywords": [
		"css",
		"esm",
		"esbuild"
	],
	"homepage": "https://github.com/esmcss/esmcss#readme",
	"bugs": {
		"url": "https://github.com/esmcss/esmcss/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/esmcss/esmcss.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"esmcss_esbuild_plugin",
		"var__css__replace"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"esbuild": "^0.20.0"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": ".npm"
	},
	"sideEffects": false
}